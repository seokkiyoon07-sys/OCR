# SNar OCR 환경 변수 설정

# 애플리케이션 설정
NEXT_PUBLIC_APP_NAME=SNar OCR
NEXT_PUBLIC_APP_URL=https://snar-ocr.com
NEXT_PUBLIC_APP_VERSION=1.0.0

# API 설정
NEXT_PUBLIC_API_URL=https://api.snar-ocr.com
API_SECRET_KEY=your-secret-key-here

# 데이터베이스 설정
DATABASE_URL=postgresql://username:password@localhost:5432/snar_ocr
DATABASE_POOL_SIZE=10

# 파일 업로드 설정
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf
UPLOAD_DIR=./uploads

# 보안 설정
JWT_SECRET=your-jwt-secret-here
ENCRYPTION_KEY=your-encryption-key-here
CORS_ORIGIN=https://snar-ocr.com

# 외부 서비스
GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
GOOGLE_VERIFICATION=your-google-verification-code

# 개발/프로덕션 설정
NODE_ENV=development
NEXT_PUBLIC_DEBUG=false
